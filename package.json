{
  "name": "ookive",
  "version": "0.0.2",
  "description": "Permanent persistent storage, clientside and serverside",
  "main": "build/ookive.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "devDependencies": {
    "apage": "latest",
    "coffee-script": "latest",
    "nodemon": "latest"
  },
  "dependencies": {
    "ookonsole": "latest"
  },
  "scripts": {
    "hi": "open .; open index.html; open test/run-test.html; subl .",
    "make": "node bin/akaybe-build",
    "index": "node_modules/apage/bin/apage -i README.md doc/*.md -o index.html -p \"`cat node_modules/apage/build/plugin/*.js`\"",
    "test": "node test/run-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richplastow/ookive.git"
  },
  "keywords": [
    "undo",
    "history",
    "archive",
    "database",
    "notifications",
    "push",
    "ws",
    "websockets",
    "persistent",
    "localstorage",
    "CoffeeScript"
  ],
  "author": "Rich Plastow <rich@richplastow.com> (http://richplastow.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/richplastow/ookive/issues"
  },
  "homepage": "http://ookive.richplastow.com/"
}
